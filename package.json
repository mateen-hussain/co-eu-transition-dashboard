{
  "name": "dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_PATH=. node server.js",
    "dev": "NODE_PATH=. nodemon server.js",
    "test:lint": "eslint .",
    "test:unit": "NODE_PATH=. FEATURE_2FA=true mocha 'test/unit/**/*.test.js' --opts 'test/unit/mocha.opts'"
  },
  "nodemonConfig": {
    "ignore": [
      "common/*"
    ]
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-nunjucks": "^2.2.3",
    "express-winston": "^4.0.3",
    "glob": "^7.1.6",
    "govuk-frontend": "^3.5.0",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "node-xlsx": "^0.15.0",
    "nunjucks": "^3.2.1",
    "nunjucks-await-filter": "0.0.1",
    "nunjucks-date-filter": "^0.1.1",
    "nyc": "^15.0.0",
    "passport": "^0.4.1",
    "passport-2fa-totp": "0.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.4.4",
    "qrcode": "^1.4.4",
    "sequelize": "^5.21.5",
    "speakeasy": "^2.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-jq": "0.0.9",
    "copy-webpack-plugin": "^5.1.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.0",
    "mock-require": "^3.0.3",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "notifications-node-client": "^4.7.2",
    "proxyquire": "^2.1.3",
    "randomstring": "^1.1.5",
    "sass-loader": "^8.0.2",
    "sequelize-test-helpers": "^1.2.2",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "webpack": "^4.42.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "resolutions": {
    "braces": ">=2.3.1",
    "assign-deep": ">=1.0.1",
    "acorn": ">=7.1.1"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-directory": "./coverage/.nyc_output",
    "check-coverage": true,
    "per-file": true,
    "lines": 60,
    "statements": 60,
    "functions": 60,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "instrument": true,
    "exclude": [
      "test/**/*",
      "services/logger.js",
      "middleware/logger.js",
      "pages/filters-example/**/*.js",
      "pages/admin/**/*.js"
    ]
  }
