{
  "name": "dashboard",
  "version": "1.16.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_PATH=. node server.js",
    "dev": "NODE_PATH=. nodemon server.js",
    "install": "bash build.sh",
    "test:lint": "eslint .",
    "test:unit": "NODE_PATH=. FEATURE_2FA=true mocha 'test/unit/**/*.test.js' --opts 'test/unit/mocha.opts'",
    "release": "release-it --no-npm.publish --ci",
    "deploy-staging": "bash deploy.sh staging staging.yml",
    "deploy-research": "bash deploy.sh research research.yml",
    "deploy-production": "bash deploy.sh production production.yml"
  },
  "nodemonConfig": {
    "ignore": [
      "common/*"
    ]
  },
  "npm": {
    "publish": false
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chart.js": "^2.9.3",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "copy-webpack-plugin": "^5.1.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.5.3",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-nunjucks": "^2.2.5",
    "express-winston": "^4.0.3",
    "getopts": "^2.2.5",
    "glob": "^7.1.6",
    "govuk-frontend": "^3.6.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.25.1",
    "mysql2": "^2.1.0",
    "node-cron": "^2.0.3",
    "node-sass": "^4.14.0",
    "notifications-node-client": "^4.7.3",
    "nunjucks": "^3.2.1",
    "nunjucks-await-filter": "0.0.1",
    "nyc": "^15.0.1",
    "passport": "^0.4.1",
    "passport-2fa-totp": "0.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.5.2",
    "qrcode": "^1.4.4",
    "randomstring": "^1.1.5",
    "readline-sync": "^1.4.10",
    "request-promise-native": "^1.0.8",
    "sass-loader": "^8.0.2",
    "sequelize": "^5.21.7",
    "speakeasy": "^2.0.0",
    "sprintf-js": "^1.1.2",
    "umzug": "^2.3.0",
    "uuid": "^7.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "winston": "^3.2.1",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-jq": "0.0.9",
    "eslint": "^6.8.0",
    "mocha": "^7.1.2",
    "mock-require": "^3.0.3",
    "nodemon": "^2.0.3",
    "proxyquire": "^2.1.3",
    "release-it": "^13.5.8",
    "sequelize-test-helpers": "^1.2.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "resolutions": {
    "yargs-parser": "18.1.2"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-directory": "./coverage/.nyc_output",
    "check-coverage": true,
    "per-file": true,
    "lines": 60,
    "statements": 60,
    "functions": 60,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "instrument": true,
    "exclude": [
      "test/**/*",
      "services/logger.js",
      "middleware/logger.js",
      "migrations/**/*"
    ]
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
