{
  "cookie": {
    "secret": "COOKIE_SECRET"
  },
  "env": "NODE_ENV",
  "services": {
    "mysql": {
      "uri": "DATABASE_URL"
    },
    "tableau": {
      "url": "TABLEAU_URL",
      "whiteListedIpRange": {
        "__name": "TABLEAU_WHITELIST_IP_RANGE",
