  },
  "port": "PORT",
  "features": {
    "twoFactorAuth": "FEATURE_2FA"
  },
  "notify": {
    "apiKey": "NOTIFY_API_KEY",
    "summaryNotificationEmail": "SUMMARY_NOTIFICATION_EMAIL"
  }
}
